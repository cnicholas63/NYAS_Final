<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chris.nyas_final" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HelplineActivity"
            android:label="@string/title_activity_contact_us" >
        </activity>
        <activity
            android:name=".AdviceServicesActivity"
            android:label="@string/title_activity_advice_services" >
        </activity>
        <activity
            android:name=".OurServicesActivity"
            android:label="@string/title_activity_our_services" >
        </activity>
        <activity
            android:name=".LegalServices"
            android:label="@string/title_activity_legal_services" >
        </activity>
        <activity
            android:name=".MyNyasLogIn"
            android:label="@string/title_activity_my_nyas_log_in"
            android:noHistory="true"> <!-- prevent this activity being returned to when back button is pressed-->
        </activity>
        <activity
            android:name=".MyNyasWelcome"
            android:label="@string/title_activity_my_nyas_welcome" >
        </activity>
        <activity
            android:name=".CalendarActivity"
            android:label="@string/title_activity_calendar" >
        </activity>
        <activity
            android:name=".AppointmentActivity"
            android:label="@string/title_activity_appointment" >
        </activity>
    </application>

</manifest>
